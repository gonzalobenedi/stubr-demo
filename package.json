        "cors-anywhere": "^0.4.3",
        "eslint": "^7.12.0",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-prettier": "^3.1.4",
        "esm": "^3.2.25",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.2",
        "stubr": "^0.4.0",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.6"
